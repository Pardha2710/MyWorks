<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:orientation="vertical"
    android:layout_width="match_parent"
    android:background="#FFF"
    android:layout_height="match_parent">

    <ImageView
        android:id="@+id/ivdress"
        android:layout_width="150dp"
        android:layout_height="145dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_marginStart="18dp"
        android:layout_marginTop="25dp"
        android:clickable="true"
        app:srcCompat="@drawable/dresses" />

    <TextView
        android:id="@+id/textView6"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_marginStart="56dp"
        android:layout_marginTop="177dp"
        android:text="Dresses"
        android:textColor="#000"
        android:textSize="18sp" />

    <ImageView
        android:id="@+id/ivjackets"
        android:layout_width="171dp"
        android:layout_height="155dp"
        android:layout_alignParentTop="true"
        android:layout_alignParentEnd="true"
        android:layout_marginTop="23dp"
        android:layout_marginEnd="30dp"
        android:clickable="true"
        app:srcCompat="@drawable/jackets" />

    <TextView
        android:id="@+id/textView7"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentTop="true"
        android:layout_alignParentEnd="true"
        android:layout_marginTop="186dp"
        android:layout_marginEnd="92dp"
        android:text="Jackets"
        android:textColor="#000"
        android:textSize="18sp" />

    <ImageView
        android:id="@+id/ivjeans"
        android:layout_width="151dp"
        android:layout_height="177dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentBottom="true"
        android:layout_centerVertical="true"
        android:layout_marginStart="15dp"
        android:layout_marginBottom="342dp"
        android:clickable="true"
        app:srcCompat="@drawable/jeans" />

    <TextView
        android:id="@+id/textView8"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_alignParentBottom="true"
        android:layout_marginStart="68dp"
        android:layout_marginBottom="311dp"
        android:text="Jeans"
        android:textColor="#000"
        android:textSize="18sp" />

    <ImageView
        android:id="@+id/ivshirts"
        android:layout_width="171dp"
        android:layout_height="183dp"
        android:layout_alignParentEnd="true"
        android:layout_alignParentBottom="true"
        android:layout_marginEnd="28dp"
        android:layout_marginBottom="342dp"
        android:clickable="true"
        app:srcCompat="@drawable/shirts" />

    <TextView
        android:id="@+id/textView9"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentEnd="true"
        android:layout_alignParentBottom="true"
        android:layout_marginEnd="91dp"
        android:layout_marginBottom="310dp"
        android:text="Shirts"
        android:textColor="#000"
        android:textSize="18sp" />

    <ImageView
        android:id="@+id/ivskirts"
        android:layout_width="152dp"
        android:layout_height="175dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentBottom="true"
        android:layout_marginStart="22dp"
        android:layout_marginBottom="118dp"
        android:clickable="true"
        app:srcCompat="@drawable/skirts" />

    <TextView
        android:id="@+id/textView10"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_alignParentBottom="true"
        android:layout_marginStart="79dp"
        android:layout_marginBottom="94dp"
        android:text="Skirts"
        android:textColor="#000"
        android:textSize="18sp" />

    <ImageView
        android:id="@+id/ivtrousers"
        android:layout_width="168dp"
        android:layout_height="176dp"
        android:layout_alignParentEnd="true"
        android:layout_alignParentBottom="true"
        android:layout_marginEnd="32dp"
        android:layout_marginBottom="114dp"
        android:clickable="true"
        app:srcCompat="@drawable/trousers" />

    <TextView
        android:id="@+id/textView11"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentEnd="true"
        android:layout_alignParentBottom="true"
        android:layout_marginEnd="85dp"
        android:layout_marginBottom="87dp"
        android:text="Trousers"
        android:textColor="#000"
        android:textSize="18sp" />

    <EditText
        android:id="@+id/setext"
        android:layout_width="315dp"
        android:layout_height="52dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentBottom="true"
        android:layout_marginStart="26dp"
        android:layout_marginBottom="20dp"
        android:background="@drawable/bg_txt_empty"
        android:ems="10"
        android:hint="Search Company"
        android:inputType="textPersonName"
        android:paddingLeft="20dp" />

    <Button
        android:id="@+id/search"
        android:layout_width="50dp"
        android:layout_height="wrap_content"
        android:layout_alignParentEnd="true"
        android:layout_alignParentBottom="true"
        android:layout_marginEnd="20dp"
        android:layout_marginBottom="25dp"
        android:background="@drawable/ic_search"
        android:text=" " />
</RelativeLayout>