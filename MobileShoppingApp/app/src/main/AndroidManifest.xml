<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.mobileshopping">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="com.example.mobileshopping.FinalScreen"></activity>
        <activity android:name="com.example.mobileshopping.CustomerInfo" />
        <activity android:name="com.example.mobileshopping.DataPlans" />
        <activity android:name="com.example.mobileshopping.GoogleModels" />
        <activity android:name="com.example.mobileshopping.SamsungModels" />
        <activity android:name="com.example.mobileshopping.IphoneModels" />
        <activity android:name="com.example.mobileshopping.Brands" />
        <activity android:name="com.example.mobileshopping.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>